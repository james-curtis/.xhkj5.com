@charset "utf-8";

/* common */
* {word-wrap:break-word;}
ul,ol,li,span,p,form,h1,h2,h3,4,h5,h6,dl,dt,dd,fieldset,.pr,.pc {margin:0;padding:0;border:0;z-index:inherit;}
img,a img { border:0; margin:0; padding:0;}
ul,ol,li { list-style:none;}
*{margin:0; padding:0;}
html,body { min-height:100%; font: 12px/1.6 STHeiti,'Microsoft YaHei',Helvetica,Arial,sans-serif;color:#4a4a4a;margin:0; padding:0;}
input,select,textarea,button { font:14px/1.5 Microsoft YaHei, Helvetica, sans-serif;margin:0;padding:0; border:0;}
h1, h2, h3, h4, h5, h6 { font-size: 1em;}
em, cite, i { font-style: normal;}
label { cursor: pointer;}
*:focus {outline: none;resize: none;}
a:link,a:visited,a:hover { color:#131313; text-decoration:none;}
.blue, a.blue:link, a.blue:visited, a.blue:hover { color:#0086CE; text-decoration:none;}
.grey, a.grey:link, a.grey:visited, a.grey:hover { color:#9C9C9C; text-decoration:none;}
.orange, a.orange:link, a.orange:visited, a.orange:hover {color:#F60;text-decoration:none}
.z { float: left;} .y { float: right;}
.cl:after { content: "."; display: block; height: 0; clear: both; visibility: hidden;}
.cl { zoom: 1;}
.vm { vertical-align: middle;}
.rq { color: red;}
.bl_none { border-bottom:0 !important;}
.mtn { margin-top: 5px !important;}
.mbn { margin-bottom: 5px !important;}
.bg { background: #f4f5f6;}
.blockBG {background: #fff !important;}
section {margin-bottom:10px; background:#fff; overflow:hidden;}
.nodata {padding:15px;text-align: center;font-size:14px;}

/*header*/
.header {width:100%;height:44px;}
#hd {position:fixed; top:0px;background:#fe3a3a;width:100%;height:44px; text-align:center; overflow:hidden; z-index:999;}
#hd, #hd a {color:#fff;}
.hdc{ position:relative; padding-left:44px; padding-right:44px; height:44px;}
.hdc a { display:block; font-size:26px;line-height: 44px;color:#fe3a3a;}
.hdc .hd_left, .hdc .hd_right {display:inline-block; position:absolute; top:0; width:45px;line-height: 44px;}
.hdc .hd_left {top:0; left:0;}
.hdc .hd_name {width:100%; display:inline-block; text-align:center;height:44px; line-height:44px; vertical-align:top;}
.hdc .hd_name h2 { padding:0px; text-align:center; color:#fff;height:44px; font-size:20px; font-weight:100;}
.hdc .logo {width:100%; display:inline-block; text-align:center;height:44px; line-height:44px; vertical-align:top;}
.hdc .logo h2 { padding:0px; text-align:center; color:#242424;height:44px;}
.hdc .logo a {height:100%;}
.hdc .logo img {height:100%;}
.hdc .hd_right { top:0; right:0;}
.hdc .hd_btn { top:0; right:5px;line-height:26px;}
.hdc .hd_tab { width:auto;}
.hdc1{ position:relative; padding:0 10px; height:44px;}
.hdc1 .logo { position:absolute; left:10px; top:0;width:100px; display:inline-block; text-align:center;height:44px;vertical-align:top;}
.hdc1 .logo img {width:100%;height:auto;}
.hdc1 .scbar { position:absolute; top:0; left:110px; right:0; display:inline-block;height:44px; line-height:44px; vertical-align:top;}
.hdc1 .scbar div { height:30px;line-height:30px; display:block; margin:7px 10px; padding:0 5px; background:#fff; border-radius:5px; color:#d8d8d8 !important; text-align:left; font-size:14px;padding:0 0 0 25px; box-sizing:border-box;}
.hdc1 .scbar div.icon-search:before {margin-left:-20px; float:left; font-size:18px;}
.hdc1 .scbar .hdform { position:relative; float:left; height:30px;width: 100%; overflow:hidden;}
.hdc1 .scbar .hdform .input {width:100%; height:30px; border-radius:5px;}
.hdc1 .scbar .hdform .hdform_submit { position:absolute; top:0; right:0; height:30px; background:none; width:49px;margin-left:-50px; border-left:1px solid #d8d8d8; color:#999;}
.top { position:fixed;top:0; width:100%;z-index: 999;}
.bar {height:81px;}

#top_nav{background:#fdfdfd;border-bottom:1px #ddd solid;height:36px;line-height:36px;overflow:hidden; position:fixed; top:44px; z-index:999;}
#top_nav{width:100%;}
.top_menu_bar {height: 36px;line-height: 36px;position:relative; padding-right:42px;}
.top_menu_more{position: absolute;width:36px; height: 36px; line-height: 36px;padding-left:6px; text-align: center;right:0px; opacity: 1; z-index: 100;background: url(../images/topnav_line.png) no-repeat center left;background-size: contain;background-color: rgba(244,245,246,.3);}
.top_menu_more a{display: block; color: #999 !important;font-size: 18px;}
.top_menu_more i{font-size:24px;}
.top_menu_list {overflow: hidden;line-height:36px;font-size: 14px;}
.top_menu_list .nav_list{overflow: hidden; width: 900px;height:36px;}
.top_menu_list .nav_list ul{display: inline-block;}
.top_menu_list .nav_list li{float:left;overflow: hidden;}
.top_menu_list .nav_list li a{display: block;color:#333; padding:0px 8px;}
.top_menu_list .nav_list li a:active{background-color: rgba(255,255,255,.3);}
.top_menu_list .nav_list li a:hover{background-color: rgba(255,255,255,.3);}
.top_menu_list .nav_list li.a a{color: #333;}
#navs_scrolls {z-index: 1;height: 36px;width: 100%;overflow: hidden;-ms-touch-action: none;}

.pop_menu_box { position:fixed; top:0; left:0; right:0; width:100%; height:100%; background:#f1f1f1; padding-top:40px; z-index:1000;}
.exit { position:absolute; top:10px; right:10px;width:30px; height:30px; line-height:30px;border-radius:15px; text-align:center; font-size:16px;color:#222;}
.pop_list { margin:0 10px 20px 10px;}
.pop_list .hd { height:40px;line-height:40px;padding:0 5px;font-size:12px;font-weight:100;color: #666;overflow:hidden;}
.pop_list .bd { font-size:14px;overflow:hidden;}
.pop_list .bd li { float:left; display:inline-block; width:50%;box-sizing:border-box;}
.pop_list .bd li a { display:block; padding:0 10px; margin:0 5px 10px 5px; height:36px; line-height:36px; border:1px solid #f8f8f8;box-sizing:border-box;background:#fff;overflow:hidden; text-align:center;font-size:14px;color:#666;}
.pop_list .bd li.on a{background-color: #fff;color:#F00;}
.pop_list .bd.t3 li { float:left; display:inline-block; width:33.3%;box-sizing:border-box;}
.pop_list .bd.t4 li { float:left; display:inline-block; width:25%;box-sizing:border-box;}

.tabs {position:relative; overflow:hidden;}
.tabs .hd { height:39px;  border-bottom:1px solid #f4f5f6;}
.tabs .hd ul {}
.tabs .hd ul li { float:left; display:inline-block;text-align:center; box-sizing:border-box;}
.tabs .hd ul li a { display:inline-block; font-size:14px;color:#333;line-height:40px; height:37px;}
.tabs .hd ul li.on {}
.tabs .hd ul li.on a { color:#fe3a3a; border-bottom:3px solid #fe3a3a;}
.tabs .hd ul.tb2 li { width:50%;}
.tabs .hd ul.tb3 li { width:33.3%;}
.tabs .hd ul.tb3 li:first-child { width:33.4%;}
.tabs .hd ul.tb4 li { width:25%;}
.tabs .hd ul.tb5 li { width:20%;}
.tabs .bd { width:100%; overflow:hidden;}

.visitclienttip, .clew_con { background:#333; color:#fff; padding:10px;border-bottom:1px solid #f4f5f6; overflow:hidden;}
.visitclienttip p  { line-height:20px; padding:5px 0;}
.visitclienttip a.btn_download { background:#fe3a3a; border-radius:5px; float:right; display:block; width:70px; text-align: center; line-height:24px; color:#E9EFF3; margin-top:3px; margin-left:5px;}
.visitclienttip a.btn_download:hover {background:#3094CF;}
.clew_con h2 {line-height:30px; font-size:16px;}
.clew_con p { margin-bottom:30px;}
.clew_con .button {background:#fe3a3a; border-radius:5px; margin-left:10px; width:80px; height:24px; text-align: center; line-height:24px; color:#fff;font-size:12px;}
.clew_con .button:hover {background:#3094CF;}

/*----- forumlist -----*/
.forum_box{ background:#fff;}
.forum_box .sub_title{border-bottom:1px #e8e8e8 solid;; height:36px; line-height:36px; white-space: nowrap; overflow: hidden;}
.forum_box .sub_title h2 {font-size:16px; font-weight:700; margin-left:10px;}
.bm_c {}
.bm_h .o { float: right; width: 31px;}
.bm_h .o img { float: right; cursor: pointer;}
.bm_h .i { padding-left: 10px;}
.bm_h .pn { margin-top: 4px;}
.bm_h { border-bottom:1px #e8e8e8 solid; background:#f8f8f8; height:35px; line-height:36px; white-space: nowrap; overflow: hidden;}
.bm_h h2 { height: 35px; line-height:38px;  font-size:14px; font-weight:100; color:#727272; margin-left:10px;}
.bm_h:hover {}
.sub_forum {padding:0px;}
.sub_forum ul {overflow:hidden; position:relative;}
.sub_forum li {border-bottom:1px solid #e8e8e8;border-left:1px solid #e8e8e8; font-size:16px; overflow:hidden; margin-left:-1px; height:69px; position:relative;}
.sub_forum li a {color:#232323;display:block; padding:5px 10px;} 
.sub_forum li .fl_icn_g {width:48px; height:48px; position:relative; top:5px; left:4px;} 
.sub_forum li .fl_icn_g  img { width:48px; height:48px; border-radius:0px;} 
.sub_forum li dl { display:block; float:left; position:absolute; top:5px; padding-left:65px;} 
.sub_forum li dt { line-height:26px;height:26px; margin-bottom:5px; overflow:hidden;} 
.sub_forum li dd { line-height:14px;height:14px; margin-right:10px; font-size:12px; color:#b4b4b4; overflow:hidden;} 
.sub_fw ul {overflow:hidden; position:relative; padding:6px;}
.sub_fw li {font-size:12px; margin:3px; float:left; display:inline-block; height:26px; line-height:26px; position:relative; padding:0 5px; box-sizing:border-box;background:#e8e8e8;border-radius:5px;text-align:center;}
.sub_fw li a {color:#232323; overflow:hidden;} 

.forumlistpbl_box{width:100%;padding-bottom:10px; overflow:hidden;}
.forumlistpbl_box .pbnv{ line-height:40px; font-size:14px; margin:0 3%;}
.forumlistpbl{overflow: hidden;margin: 0 auto 20px auto;width: 96%;border: solid;border-color: #ddd;border-width: 1px 1px 1px 0;
background: #fff;}
.forumlistpbl li{float: left;overflow-x: hidden;overflow-y: auto;padding: 5px 1.5%;width: 30%;height: 300px;border-left: 1px solid #CCC;}
.forumlistpbl li p{height:30px; line-height:30px;}
.forumlistpbl li a{display: block;white-space: nowrap;overflow: hidden;padding: 0 4px;text-decoration: none;border-width: 1px 0; font-size:14px; color:#333;text-overflow: ellipsis;}
.forumlistpbl li .pbls a{background: #ddd;}
.forumlistpbl_box .pbut{width: 50%;margin: 0 25% 10px 25%; display:block;}
.forumlistpbl_box .pn{background: #CFCFCF;padding: 3px 28px;border: none;width: 100%;height: 35px;line-height: 32px;}
.forumlistpbl_box .pnc{background:#F29337;padding: 3px 28px;border: none;width: 100%;height: 35px;line-height: 32px; color:#fff;}
	
/*forumdisplay*/
.fheader{position:relative;padding:10px 10px 10px 80px;;overflow:hidden;height:65px;background:url(../images/forumdisplay_bg.jpg);color:#fff;}
.fheader .fhlogo{position:absolute;top:10px;left:10px;display:block;width:45px;height:45px; padding:10px;overflow:hidden;}
