.login_from input, .login_from .questionli, .login_from .user_seccode { background:#fff;color:#323232; border:#d0d0d0 1px solid;}
.login_from li input { width:100%; box-sizing:border-box;line-height: 20px;padding:10px;color:#323232;}
.login_from .questionli { height: 20px; padding: 10px 20px;line-height: 20px;position: relative;}
.login_from .questionli:after { content: '';display: inline-block;width: 6px;height: 6px;border-style: solid;border-width: 0px 0px 1px 1px;-webkit-transform: rotate(-45deg);-moz-transform: rotate(-45deg);-ms-transform: rotate(-45deg);-o-transform: rotate(-45deg);transform: rotate(-45deg);position: absolute;right: 22px;bottom: 19px;}
.login_select { display: block; position: relative;}
.login_select select {width: 100%;position: absolute;top: 0;left: 0;z-index: 2;cursor: pointer;height: 20px;opacity: 0.0001;background: none;-moz-appearance: none;-webkit-appearance: none;appearance: none;border-radius:20px;}
.sel_list { width:100%;border: 0; padding: 2px 4px; font-size:14px; font-weight:700;}

.login_pop { background:#F1F1F1; padding: 0 14px 20px;}	
