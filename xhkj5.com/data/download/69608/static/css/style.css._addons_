.plc .pi .quote{ padding:8px; margin-top:10px; border:1px solid #d9d9d9; background:#e6e6e6; border-radius:5px; font-size:12px; line-height:150%;color:#666;}
.plc .pi .quote a{ color:#888; margin:0 5px;}
.plc .pi .img_list { margin:4px auto;}	
.plc .pi .img_list li { float:left; margin:0; text-align:center;vertical-align: middle;}
.plc .pi .img_list li a { display:block;}
.replaytitle{line-height:30px;height:30px;margin-top:10px;margin-bottom:10px;position:relative;z-index:1; border-bottom:1px solid #e8e8e8;}
.replaytitle a{display:inline-block;color:#fe3a3a; font-size:14px;height:28px; border-bottom:3px solid #fe3a3a;}

.manage { margin:4px -10px 0 0; position:absolute; top:38px; right:5px; height:50px;}
.dialogbox .button { margin:5px;}
.button { margin-top:5px;}
.button,.button2 { width:50px; height:32px; color:#FFF; background:#fe3a3a; border:0; font-size:14px;border-radius:5px;}
.plc_replay { margin:0 -15px; background:#f5f5f5; padding:0 15px;}
.plc_replay .avatar {left:15px;}
.fastpost {margin-left:45px;} 
.fastpost .input { width:100%; height:34px; text-indent:7px; margin-top:2px; border:1px solid #D9D8D8; box-sizing:border-box; border-radius:5px;} 
.search .input { width:100%; height:30px; text-indent:7px; margin-top:2px; border:1px solid #D9D8D8;}
	
/*page*/
.page { text-align:center; margin:20px 10px;}	 
.page a { border:1px solid #D7D7D7; border-radius:5px; padding:5px 9px; background:#FFF; margin:0 4px;} 
.page select{ border: 1px solid #CCC; height: 28px;}

/*userinfo*/
.us_avt { height:100px; background:#fe3a3a; text-align:center;}
.us_avt img { border-radius:45px;border:0px solid #FFF;box-shadow: 1px 2px 2px rgba(0,0,0,0.3);width:50px;height:50px;margin-top:10px;}
.us_avt .name { color:#fff; font-size:14px; line-height:30px;}
.usbox { border:1px solid #e8e8e8; margin:10px -1px;}
.usbox li { line-height:38px; border-bottom:1px solid #e8e8e8; padding:0 20px 0 15px; color:#323232; font-size:14px; font-weight:700;}
.usbox li:last-child { border-bottom:0px;}
.usbox li span { color:#fe3a3a; float:right;}
.logout_but { margin:10px 0 ;}
.logout_but a { height:40px; line-height:40px; overflow:hidden; color:#fe3a3a; font-size:14px; text-align:center; border:0; display:block;}
.list_user_menu { margin-top:10px;}
.list_user_menu li { position:relative; border-bottom: 1px solid #e8e8e8; height:39px; line-height:40px; font-size:16px; padding-left:15px;}
.list_user_menu li:before {display:inline-block; margin:0 15px; color:#727272;}
.list_user_menu li i { float:right; margin-right:15px;display:block;}
.tit_msg { position: relative;}
.tit_msg img { position:absolute; left:108px; top:28px;}
.msgbox{ padding:10px;}
.friend_msg { padding:5px 0;}
.friend_msg .avat img,.self_msg .avat img{ padding:1px; background: #F2F2F2; border: 1px solid #BABABA; display: inline-block;}
.self_msg { padding:5px 0;}
.friend_msg .date { color:#fff; padding-left:12px;}
.self_msg .date { color:#999; padding-right:12px; text-align:right;}
.dialog_green, .dialog_white {padding: 10px;border-radius: 5px;position: relative; width:60%;}	
