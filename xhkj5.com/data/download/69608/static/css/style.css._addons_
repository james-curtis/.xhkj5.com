.fheader h1{height:30px;line-height: 30px; font-size:16px;margin-top:8px;}
.fheader p{line-height:15px;height:15px;font-size:12px; overflow:hidden;}
.fheader p.info span{margin-right:15px;line-height:15px;}
.fheader p.info em {}
.fheader .forum_fav { position:absolute; top:30px; right:20px; display:inline-block; height:30px; line-height:32px; border:1px solid #bbb; border-radius:5px; color:#bbb; font-size:14px;padding:0 10px;}
.hot_label,.dis_label,.pic_label {display: inline-block;font-size: 9px;min-width: 12px;height: 12px;text-align: center;border: 1px solid #fff;border-radius: 2px;}
.threadlist { overflow:hidden;}
.threadlist .hot_label,.threadlist .dis_label,.threadlist .pic_label {padding:0 2px;}
.article_link {display:block; position:relative;margin:0 15px; padding:15px 0;overflow: hidden;}
.article_link h3 { line-height:21px; font-size:16px;font-weight: 400;overflow: hidden; color:#222;}
.article_link:visited h3 { color:#aaa;}
.article_link h3.mr5 { margin-right:10px;}
.article_link .thumb {}
.article_link .thumb img { width:100%;border: 0;}
.article_link .thumbig {height: 12rem; margin-top:5px; overflow:hidden;}
.article_link .thumb1 {overflow: hidden; width: 33%;display: inline-block; position:relative; vertical-align:top;}
.article_link .desc {overflow: hidden;float: left; width:67%;display: inline-block; position:relative; vertical-align:top;}
.article_link .item_info { font-size:12px; overflow:hidden; color:#999;margin-top:5px;}
.article_link .item_info span {display: inline-block;line-height: 12px;vertical-align: middle;}
.article_link .item_info .space {margin-right: 5px;}
.article_link .item_info .hot_label {color: #f85959;line-height: 14px;border-color:#f85959;}
.article_link .item_info .dis_label {color: #2a90d7;line-height: 14px;border-color:#2a90d7;}
.article_link .item_info .pic_label {color: #009b07;line-height: 14px;border-color:#009b07;}
.one-px-border {border-top: 1px solid rgba(221,221,221,.6);margin: 0 15px;}

/*post*/
.btn_pn { position:absolute; right:5px; display:inline-block; padding:0; width:50px; height:44px; text-align:center; overflow:hidden; border:0; line-height:44px; font-size:16px;}
.btn_pn_grey { background:#fe3a3a; color:#ddd;}
.btn_pn_blue { background:none; color:#fff;}
.post_from{ padding:10px;}
.post_msg_from{ padding:10px;}
