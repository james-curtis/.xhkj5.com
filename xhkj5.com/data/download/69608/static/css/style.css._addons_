.pmbox li .time { float:right;}

/*login*/
.loginbox {min-height:100%; padding:30px;}
.login_header img { max-width:100%;}
.login_from {border-radius:8px;}
.login_from li { margin:5px 0; padding: 5px 0px; color:#323232;}
