.post_imglist li .del { position: absolute; left:-5px; top:-10px;}
.post_imglist li .p_img img { padding:2px; border:1px solid #DDD;}

/*viewthread*/
.postlist { padding:10px 15px; background:#fff;}
.postlist h2 { clear:both; font-size:22px; line-height:1.5em; font-weight:bold; padding-top:10px; color:#222;}
.plc {position:relative;}
.plc .avatar { position:absolute; left:0px; top:11px;  display:block;}
.plc .avatar img{border-radius:16px; width:32px; height:32px;}
.plc .pi { padding:10px 0; position:relative; display:block;}
.plc .pi .authi{ position:relative; margin-left:40px; margin-bottom:10px;}
.plc .pi .authi li em{ float:right;}	
.plc .pi .message{ font-size:14px; line-height:150%;}
.plc .pi .message .jammer{ font-size:10px; color:#f0f0f0;}
