.searchform {padding:20px;}
.thread_tit {height:40px; line-height:40px; border:1px solid #e8e8e8; border-left:0px; border-right:0px; background:#f1f1f1; padding:0 15px;}

/*footer*/
.footer {text-align:center; line-height:30px; color:#fff; padding:10px 0 0 0;background:#fe3a3a;}
.footer a {display:inline-block; margin:0 10px; height:26px; line-height:26px;color:#fff;}
.footer a.a {padding:0 10px; border-radius:5px;background:#fff;color:#666;}

#bottom {display:block; width:100%; height:45px; line-height:20px;}
#footbar {position:fixed; bottom:0; z-index:9999; width:100%; height:44px;}
#footbar .fbc {height:44px; border-top: #e8e8e8 0px solid; background:#f4f5f6;}
#footbar .fbc li {float:left; width:25%; text-align:center;color:#2a2a2a;padding-top:2px;}
#footbar .fbc li a {color:#2a2a2a; display:block;font-size:12px; line-height:15px;height:44px;}
#footbar .fbc li a:before {display:block; height:24px; font-size:20px; line-height:24px;font-size:22px;}
#footbar .fbc li.a a {color:#fe3a3a;}

.scrolltop { display:block; position:fixed; right:8px; bottom:68px; height:48px; width:48px; z-index:99; background:rgba(64, 64, 64, .9); border-radius:50%; box-shadow:0 0 2px rgba(0,0,0,.3);}
.scrolltop:after { content:""; position:absolute; top:16px; left:15px; width:18px; height:3px; border-radius:1px; background:white; border-radius:1px;}
.scrolltop:before { content:""; position:absolute; top:20px; left:15px; display:inline-block; border-left:9px solid transparent; border-right:9px solid transparent; border-bottom:9px solid white; border-top:0; -webkit-transform:scale(1,1.2222); -webkit-transform:translate(0,1px); transform:scale(1,1.2222); transform:translate(0,1px);}
.scrolltop.bottom {}
.scrolltop.bottom:after { top:28px;}
.scrolltop.bottom:before { border-top:9px solid white; border-bottom:0; top:17px;}

.jump_c { padding:25px; font-size:15px; margin:100px 30px; background:#fff;}
.jump_c a {color:#2a90d7;}
.tip { margin:0 auto;border:1px solid #d7d7d7;width:240px;background:#f4f5f6;-moz-border-radius:5px;-webkit-border-radius:5px;border-radius:5px;text-align:center;}
.tip dt { padding:20px;font-size:14px;overflow:hidden;}  
.tip dd { background:#fff;border-top:1px solid #d7d7d7;padding:5px;overflow:hidden;}
.tip dd a { margin-left:15px;}
